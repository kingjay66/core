    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "prettier": {
    "semi": false,
    "printWidth": 60,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
